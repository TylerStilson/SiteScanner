
{
    "name": "Site Scanner",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "extension.html",
        "default_icon": "logo.jpg"
    },
    "icons": {
        "128": "logo.jpg"
    },
    "permissions": ["tabs", "<all_urls>", "nativeMessaging"],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]

}